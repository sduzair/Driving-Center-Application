<!-- Validation Errors -->
<% if( errors != null && errors.length > 0 ) { %>
<% for( var i = 0; i < errors.length; i++ ) { %>
<script>
  toastr.error("<%= errors[i] %>");
</script>
<% } %>
<% } %>
<!-- Server Messages -->
<% if( serverMsgs != null && serverMsgs.length > 0 ) { %>
<% for( var i = 0; i < serverMsgs.length; i++ ) { %>
<script>
  toastr.success("<%= serverMsgs[i] %>");
</script>
<% } %>
<% } %>
